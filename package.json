{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "my-moto-track",
  "version": "1.0.0",
  "ngPackage": {
    "dest": "dist_pack",
    "lib": {
      "entryFile": "public_api.ts",
      "umdModuleIds": {
        "primeng": "./node_modules/primeng",
        "primeng/primeng": "./node_modules/primeng",
        "primeng/table": ".node_modules/primeng/table"
      }
    },
    "whitelistedNonPeerDependencies": [
      "."
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:casa": "concurrently \"json-server --watch moto-data.json --port 5000\" \"ng serve\"",
    "build:test": "concurrently \"json-server --watch test-data.json --routes test-routes.json --port 5001\" \"ng test\"",
    "build:pack": "ng-packagr -p package.json && cd dist_pack && npm pack && cd .."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.0.2",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "concurrently": "^4.1.0",
    "core-js": "^2.5.4",
    "http-server": "^0.11.1",
    "json-server": "^0.15.0",
    "ng-packagr": "^5.4.3",
    "pack": "^2.2.0",
    "primeicons": "^1.0.0",
    "primeng": "^7.0.5",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
